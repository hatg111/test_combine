.event-list {
	list-style: none;
	font-family: Sdswaggerttf, 'Lato', sans-serif;
	margin: 0px;
	padding: 0px;
	z-index: 1;
	min-width: 380px;
}

.event-list>li {
	background-color: rgb(255, 255, 255);
	box-shadow: 0px 0px 5px rgb(51, 51, 51);
	padding: 0px;
	margin: 0px 0px 20px;
	z-index: 1;
	min-width: 380px;
}

.event-list>li>img {
	width: 70px;
	float: left;
	z-index: 1;
}

.event-list>li>.info {
	padding-top: 5px;
	text-align: left;
	z-index: 1;
	margin-left: 80px;
}

.event-list>li>.info>.title {
	font-size: 16pt;
	font-weight: 700;
	z-index: 2;
}

.event-list>li>.info>.desc {
	font-size: 13pt;
	font-weight: 300;
	z-index: 2;
	margin-bottom: 8pt;
}

.event-list>li>.info>ul {
	display: table;
	list-style: none;
	margin: 0px 0px 0px;
	padding: 0px;
	text-align: center;
	z-index: 2;
}

.event-list>li>.info>ul>li {
	display: table-cell;
	cursor: pointer;
	color: rgb(30, 30, 30);
	font-size: 11pt;
	font-weight: 300;
	z-index: 2;
}

.storeimg {
	width: 70px;
	height: 70px;
}


